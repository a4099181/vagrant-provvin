<helpItems schema="maml">

<command:command xmlns:maml="http://schemas.microsoft.com/maml/2004/10" xmlns:command="http://schemas.microsoft.com/maml/dev/command/2004/10" xmlns:dev="http://schemas.microsoft.com/maml/dev/2004/10" xmlns:MSHelp="http://msdn.microsoft.com/mshelp">
<command:details><command:name>Add-WindowsDefenderExclusions</command:name>
<command:verb>Add</command:verb>
<command:noun>WindowsDefenderExclusions</command:noun>
<maml:description><maml:para>This function add Windows defender exclusions.
</maml:para>
</maml:description>
</command:details>
<maml:description><maml:para>This function in details adds exclusions for: * specified processes,
</maml:para>
<maml:para>* specified folders,
</maml:para>
<maml:para>* specified files,
</maml:para>
<maml:para>* specified extensions.
</maml:para>
<maml:para>
</maml:para>
<maml:para>The primary aim is to improve developer machine performance.
</maml:para>
</maml:description>
<command:syntax><command:syntaxItem><maml:name>Add-WindowsDefenderExclusions</maml:name>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="1" aliases="none"><maml:name>ProcessesPaths</maml:name>
<maml:Description><maml:para>Paths where specified processess should be search for.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>( ${env:ProgramFiles(x86)}, $env:ProgramW6432 )</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="2" aliases="none"><maml:name>ProcessesList</maml:name>
<maml:Description><maml:para>Processess to exclude within Windows Defender.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>( 'msbuild.exe', 'devenv.exe', 'sqlcmd.exe', 'sqllocaldb.exe', 'sqlservr.exe', 'sqlwriter.exe')</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="3" aliases="none"><maml:name>FoldersPaths</maml:name>
<maml:Description><maml:para>Paths where specified folders should be search for.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>( $env:USERPROFILE )</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="4" aliases="none"><maml:name>FoldersList</maml:name>
<maml:Description><maml:para>Folders to exclude within Windows Defender.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>( 'MyProjects', '.nuget', '.babun' )</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="5" aliases="none"><maml:name>FilesList</maml:name>
<maml:Description><maml:para>Files to exclude within Windows Defender.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>C:\pagefile.sys</dev:defaultValue>
</command:parameter>
<command:parameter required="false" variableLength="true" globbing="false" pipelineInput="False" position="6" aliases="none"><maml:name>ExtensionsList</maml:name>
<maml:Description><maml:para>File extensions to exclude within Windows Defender.
</maml:para>
</maml:Description>
<command:parameterValue required="true" variableLength="false">String[]</command:parameterValue>
<dev:type><maml:name>String[]</maml:name>
<maml:uri /></dev:type>
<dev:defaultValue>( 'config', 'cs' , 'ldf', 'lnk', 'mdf', 'ttf', 'txt', 'xml', 'log' )</dev:defaultValue>
</command:parameter>
</command:syntaxItem>
</command:syntax>
